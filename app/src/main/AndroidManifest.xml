<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.timecrafters.analyticalengine">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="org.timecrafters.analyticalengine.MainActivity"
            android:configChanges="orientation|screenSize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="org.timecrafters.analyticalengine.athena.ScoutMatchAutonomousActivity"
            android:configChanges="orientation|screenSize" />
        <activity android:name="org.timecrafters.analyticalengine.athena.ScoutMatchTeleOpActivity" />
        <activity android:name="org.timecrafters.analyticalengine.athena.ScoutTeamAutonomousActivity" />
        <activity android:name="org.timecrafters.analyticalengine.athena.ScoutTeamTeleOpActivity" />
        <activity
            android:name="org.timecrafters.analyticalengine.athena.TeamStatisticsActivity"
            android:label="@string/title_activity_team_statistics"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>