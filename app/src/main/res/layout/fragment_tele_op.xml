                android:layout_below="@+id/textView81"
                android:layout_alignStart="@+id/textView81" />

            <TextView
                android:text="Vortex Success Percentage:"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView96"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:layout_below="@+id/textView95"
                android:layout_alignStart="@+id/textView95" />

            <TextView
                android:text="Corner Success Percentage:"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView71"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:layout_below="@+id/textView96"
                android:layout_alignStart="@+id/textView96" />

            <TextView
                android:text="N/A"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/particles_vortex_success_percentage"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:layout_below="@+id/particles_missed_corner"
                android:layout_alignParentEnd="true" />

            <TextView
                android:text="N/A"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/particles_corner_success_percentage"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:layout_below="@+id/particles_vortex_success_percentage"
                android:layout_alignParentEnd="true" />

            <TextView
                android:text="Capball"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView79"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:textStyle="normal|bold"
                android:layout_marginTop="18dp"
                android:layout_below="@+id/textView71"
                android:layout_centerHorizontal="true" />

            <TextView
                android:text="Match"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView61"
                android:textAppearance="@style/TextAppearance.AppCompat"
