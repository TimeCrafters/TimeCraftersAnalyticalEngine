<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="org.timecrafters.ftcscouting.athena.fragments.TeleOpFragment">

    <!-- TODO: Update blank fragment layout -->
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="55dp"
        android:fillViewport="true">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:text="Team Number | Team Name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:id="@+id/team"
                android:textAppearance="@style/TextAppearance.AppCompat.Headline" />

            <TextView
                android:text="TeleOp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/team"
                android:layout_centerHorizontal="true"
                android:id="@+id/textView16"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:textColor="@color/colorPrimaryDark"
                android:textStyle="normal|bold" />

            <TextView
                android:text="Beacons"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/match"
                android:layout_centerHorizontal="true"
                android:id="@+id/textView63"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:textStyle="normal|bold" />

            <TextView
                android:text="N/A"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textView63"
                android:layout_alignParentEnd="true"
                android:id="@+id/beacons_claimed"
                android:textAppearance="@style/TextAppearance.AppCompat" />

            <TextView
                android:text="N/A"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/beacons_lost"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:layout_below="@+id/beacons_claimed"
                android:layout_alignParentEnd="true" />

            <TextView
                android:text="Beacons Lost/Stolen:"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView68"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:layout_below="@+id/textView66"
                android:layout_alignStart="@+id/textView66" />

            <TextView
                android:text="N/A"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/beacons_success_percentage"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:layout_below="@+id/beacons_lost"
                android:layout_alignParentEnd="true" />

            <TextView
                android:text="Success Percentage:"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView64"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:layout_below="@+id/textView68"
                android:layout_alignStart="@+id/textView68" />

            <TextView
                android:text="Particles"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView70"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:textStyle="normal|bold"
                android:layout_marginTop="15dp"
                android:layout_below="@+id/textView64"
                android:layout_alignStart="@+id/textView63" />

            <TextView
                android:text="Scored in Corner:"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView73"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:layout_below="@+id/textView72"
                android:layout_alignStart="@+id/textView72" />

            <TextView
                android:text="Missed Vortex:"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView74"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:layout_below="@+id/textView73"
                android:layout_alignStart="@+id/textView73" />

            <TextView
                android:text="N/A"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/particles_scored_in_vortex"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:layout_above="@+id/textView73"
                android:layout_alignParentEnd="true" />

            <TextView
                android:text="N/A"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/particles_scored_in_corner"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:layout_below="@+id/particles_scored_in_vortex"
                android:layout_alignParentEnd="true" />

            <TextView
                android:text="N/A"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/particles_missed_vortex"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:layout_below="@+id/particles_scored_in_corner"
                android:layout_alignParentEnd="true" />

            <TextView
                android:text="N/A"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/particles_missed_corner"
