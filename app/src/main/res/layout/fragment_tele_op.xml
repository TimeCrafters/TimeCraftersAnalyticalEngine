            <TextView
                android:text="N/A"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/capball_success_percentage"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:layout_below="@+id/capball_missed"
                android:layout_alignParentEnd="true" />

            <TextView
                android:text="N/A"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/capball_above_crossbar"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:layout_below="@+id/capball_off_floor"
                android:layout_alignParentEnd="true" />

            <TextView
                android:text="N/A"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/capball_off_floor"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:layout_above="@+id/textView62"
                android:layout_alignParentEnd="true" />

            <TextView
                android:text="Scored in Vortex:"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView72"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:layout_below="@+id/textView70"
                android:layout_alignEnd="@+id/textView70" />

            <TextView
                android:text="Beacons Claimed:"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView66"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:layout_below="@+id/textView63"
                android:layout_alignStart="@+id/textView72" />

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/match"
                style="@style/Widget.AppCompat.Button.Small"
                android:background="@android:drawable/btn_dropdown"
                android:text="ALL"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:layout_below="@+id/textView16"
                android:layout_centerHorizontal="true" />

            <TextView
                android:text="Robot"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView32"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:textStyle="normal|bold"
                android:layout_below="@+id/textView108"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="17dp" />

            <TextView
                android:text="Dead Robot:"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView34"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:layout_below="@+id/textView32"
                android:layout_alignStart="@+id/textView108" />

            <TextView
                android:text="N/A"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/textView34"
                android:layout_alignParentEnd="true"
                android:id="@+id/robot_dead"
                android:textAppearance="@style/TextAppearance.AppCompat" />

        </RelativeLayout>
    </ScrollView>

</FrameLayout>
