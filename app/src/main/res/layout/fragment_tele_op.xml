            <TextView
                android:text="0 in dataset"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/dataset"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:layout_alignBaseline="@+id/match"
                android:layout_alignBottom="@+id/match"
                android:layout_alignParentEnd="true" />

            <TextView
                android:text="Missed Corner:"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView95"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:layout_below="@+id/textView74"
                android:layout_alignStart="@+id/textView74" />

            <TextView
                android:text="Dropped/Missed:"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView60"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:layout_below="@+id/textView105"
                android:layout_alignStart="@+id/textView105" />

            <TextView
                android:text="N/A"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/capball_capped"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:layout_below="@+id/capball_above_crossbar"
                android:layout_alignParentEnd="true" />

            <TextView
                android:text="Capped:"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView105"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:layout_below="@+id/textView62"
                android:layout_alignStart="@+id/textView62" />

            <TextView
                android:text="Capball Success Percentage:"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textView60"
                android:layout_alignStart="@+id/textView60"
                android:id="@+id/textView108"
                android:textAppearance="@style/TextAppearance.AppCompat" />

            <TextView
                android:text="Off Floor:"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView81"
                android:textAppearance="@style/TextAppearance.AppCompat"
                android:layout_below="@+id/textView79"
                android:layout_alignStart="@+id/textView71" />

            <TextView
                android:text="N/A"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/capball_missed"
                android:textAppearance="@style/TextAppearance.AppCompat"
